<Window x:Class="TheRaceTrace.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:TheRaceTrace"
        mc:Ignorable="d"
        Title="MainWindow">
    <Grid>
        <Button Command="{Binding GetLapTimesCommand}" Content="Create Trace" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="20" Width="74"/>
        <oxy:PlotView Model="{Binding Plot}" Margin="10,50,10,10"/>
    </Grid>
</Window>
